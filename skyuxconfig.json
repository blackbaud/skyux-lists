{
  "$schema": "./node_modules/@skyux/config/skyuxconfig-schema.json",
  "name": "skyux-lists",
  "mode": "easy",
  "compileMode": "aot",
  "plugins": [
    "@skyux-sdk/builder-plugin-skyux"
  ],
  "app": {
    "styles": [
      "src/app/visual-styles.scss",
      "@skyux/ag-grid/css/sky-ag-grid.css",
      "@skyux/docs-tools/css/docs-tools.css"
    ],
    "theming": {
      "supportedThemes": [
        "default",
        "modern"
      ],
      "theme": "default"
    }
  },
  "librarySettings": {
    "whitelistedNonPeerDependencies": [
      "dragula",
      "ng2-dragula"
    ]
  },
  "testSettings": {
    "e2e": {
      "browserSet": "speedy"
    },
    "unit": {
      "browserSet": "paranoid"
    }
  },
  "host": {
    "frameOptions": {
      "none": true
    }
  }
}
